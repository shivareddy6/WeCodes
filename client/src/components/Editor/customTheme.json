{
  "base": "vs-dark",
  "inherit": true,
  "rules": [
    {
      "background": "282828",
      "token": ""
    },
    {
      "foreground": "6a9955",
      "token": "comment"
    },
    {
      "foreground": "4fc1ff",
      "token": "constant"
    },
    {
      "foreground": "c8c8c8",
      "token": "entity"
    },
    {
      "foreground": "c586c0",
      "token": "keyword"
    },
    {
      "foreground": "fbde2d",
      "token": "storage"
    },
    {
      "foreground": "ce9178",
      "token": "string"
    },
    {
      "foreground": "61ce3c",
      "token": "meta.verbatim"
    },
    {
      "foreground": "9cdcfe",
      "token": "support"
    },
    {
      "foreground": "ce9178",
      "fontStyle": "italic",
      "token": "invalid.deprecated"
    },
    {
      "foreground": "f8f8f8",
      "background": "9d1e15",
      "token": "invalid.illegal"
    },
    {
      "foreground": "4ec9b0",
      "fontStyle": "italic",
      "token": "entity.other.inherited-class"
    },
    {
      "foreground": "a6a6a6",
      "token": "string constant.other.placeholder"
    },
    {
      "foreground": "dcdcaa",
      "token": "meta.function-call.py"
    },
    {
      "foreground": "7f90aa",
      "token": "meta.tag"
    },
    {
      "foreground": "7f90aa",
      "token": "meta.tag entity"
    },
    {
      "foreground": "000000",
      "token": "entity.name.section"
    },
    {
      "foreground": "4ec9b0",
      "token": "keyword.type.variant"
    },
    {
      "foreground": "dcdcaa",
      "token": "source.ocaml keyword.operator.symbol"
    },
    {
      "foreground": "dcdcaa",
      "token": "source.ocaml keyword.operator.symbol.infix"
    },
    {
      "foreground": "dcdcaa",
      "token": "source.ocaml keyword.operator.symbol.prefix"
    },
    {
      "fontStyle": "underline",
      "token": "source.ocaml keyword.operator.symbol.infix.floating-point"
    },
    {
      "fontStyle": "underline",
      "token": "source.ocaml keyword.operator.symbol.prefix.floating-point"
    },
    {
      "fontStyle": "underline",
      "token": "source.ocaml constant.numeric.floating-point"
    },
    {
      "background": "ffffff08",
      "token": "text.tex.latex meta.function.environment"
    },
    {
      "background": "7a96fa08",
      "token": "text.tex.latex meta.function.environment meta.function.environment"
    },
    {
      "foreground": "dcdcaa",
      "token": "text.tex.latex support.function"
    },
    {
      "foreground": "ffffff",
      "token": "source.plist string.unquoted"
    },
    {
      "foreground": "c586c0",
      "token": "source.plist keyword.operator"
    }
  ],
  "colors": {
    "editor.background": "#282828",
    "editor.foreground": "#D4D4D4",
    "editor.inactiveSelectionBackground": "#3A3D41",
    "editorIndentGuide.background": "#404040",
    "editorIndentGuide.activeBackground": "#707070",
    "editor.selectionHighlightBackground": "#ADD6FF26",
    "list.dropBackground": "#383B3D",
    "activityBarBadge.background": "#007ACC",
    "sideBarTitle.foreground": "#BBBBBB",
    "input.placeholderForeground": "#A6A6A6",
    "menu.background": "#252526",
    "menu.foreground": "#CCCCCC",
    "statusBarItem.remoteForeground": "#FFF",
    "statusBarItem.remoteBackground": "#16825D",
    "ports.iconRunningProcessForeground": "#369432",
    "sideBarSectionHeader.background": "#0000",
    "sideBarSectionHeader.border": "#ccc3",
    "tab.lastPinnedBorder": "#ccc3"
  }
}
